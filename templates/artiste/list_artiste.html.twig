{% extends 'base_dashboard.html.twig' %}

{% block title %}Hello DashboardController!{% endblock %}

{% block content %}
    <div class="row">
        <div class="col">
            <div class="card">
                <div class="text-center bg-secondaryColor p-3 text-white rounded-top">
                    <i class="bi bi-card-list"></i> Liste des artistes
                </div>
                <div class="card-body d-flex flex-row">
                    <div>
                        <div>
                            {% for letter in 'a'..'z' %}
                                | <a href="#{{ letter }}">{{ letter|capitalize }}</a>
                            {% endfor %}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
{% endblock %}